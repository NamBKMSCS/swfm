# Environment
DEBUG=false

# MLflow server
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_ARTIFACT_ROOT=./mlartifacts
MODELS_DIR=./models

# MLflow backend database (Postgres docker)
MLFLOW_DB_CONTAINER_NAME=swfm-mlflow-postgres
MLFLOW_DB_IMAGE=postgres:14
MLFLOW_DB_HOST=localhost
MLFLOW_DB_PORT=5432
MLFLOW_DB_NAME=mlflow
MLFLOW_DB_USER=mlflow
MLFLOW_DB_PASSWORD=mlflow

# CORS - add your Next.js app URL
CORS_ORIGINS=["http://localhost:3000","http://nextjs:3000"]

# Supabase Configuration (optional)
SUPABASE_URL=
SUPABASE_KEY=
